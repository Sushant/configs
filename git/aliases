alias ga='git add'
alias gp='git push'
alias gm='git commit -m'
alias gpu='git pull'
alias gcl='git clone'
alias gs='git status'
alias gc='git checkout'
alias gl='git log --pretty=format:"%Cgreen%h %Creset %s %Cblueby %an (%ar) %Cred %d" --graph'
alias gd='git diff --color'
alias gb="git branch \$1"
